{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "shared-node-browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "quotes": [
      "warn",
      "double"
    ],
    "linebreak-style": [
      "off"
    ],
    "comma-dangle": [
      "error",
      "never"
    ]
  }
}